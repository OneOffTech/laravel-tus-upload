{
    "name": "oneofftech/laravel-tus-upload",
    "description": "Upload files to your Laravel application with the tus.io resumable upload protocol.",
    "keywords": ["laravel", "package", "tus", "upload"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.5",
        "illuminate/auth": "6.*|7.*",
        "illuminate/console": "6.*|7.*",
        "illuminate/contracts": "6.*|7.*",
        "illuminate/database": "6.*|7.*",
        "illuminate/events": "6.*|7.*",
        "illuminate/support": "6.*|7.*",
        "illuminate/view": "6.*|7.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5|^9.0",
        "orchestra/testbench": "^4.0|^5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "OneOffTech\\TusUpload\\": "src/"
        },
        "files" : [ "src/helpers.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "brach-alias": {
            "dev-master": "0.8.x-dev"
        },
        "laravel": {
            "providers": [
                "OneOffTech\\TusUpload\\Providers\\TusUploadServiceProvider"
            ]
        }
    }
}
