{
    "name": "oneofftech/laravel-tus-upload",
    "description": "Upload files to your Laravel application with the tus.io resumable upload protocol.",
    "keywords": ["laravel", "package", "tus", "upload"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "illuminate/auth": "5.5.*|5.6.*|5.7.*",
        "illuminate/console": "5.5.*|5.6.*|5.7.*",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*",
        "illuminate/database": "5.5.*|5.6.*|5.7.*",
        "illuminate/events": "5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "illuminate/view": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5|~3.6|~3.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "OneOffTech\\TusUpload\\": "src/"
        },
        "files" : [ "src/helpers.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "brach-alias": {
            "dev-master": "0.6.x-dev"
        },
        "laravel": {
            "providers": [
                "OneOffTech\\TusUpload\\Providers\\TusUploadServiceProvider"
            ]
        }
    }
}
